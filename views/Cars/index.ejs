<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>All Cars</title>
  </head>
  <body>
    <h1>All Cars</h1>
    <table>
      <thead>
        <tr>
          <th>Car Name</th>
          <th>Car Type</th>
          <th>Car Model</th>
          <th>Car Color</th>
          <th>Brand New</th>
          <th>View</th>
        </tr>
      </thead>
      <tbody>
        <% cars.forEach(cars => { %>
        <tr>
          <td><%= cars.name %></td>
          <td><%= cars.carsType %></td>
          <td><%= cars.carsModel %></td>
          <td><%= cars.CarsColor %></td>
          <td><%= cars.NewBrand %></td>
          <td><a href="/cars/<%= cars._id %>"> View </a></td>
        </tr>
        <% }) %>
      </tbody>
    </table>
  </body>
</html>
